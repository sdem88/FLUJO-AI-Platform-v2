{
  "name": "flujo",
  "version": "0.1.1",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev -p 4200",
    "turbo": "next dev --turbopack -p 4200",
    "build": "next build",
    "start": "next start -p 4200",
    "lint": "next lint",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:4200 && electron .\"",
    "electron-build": "next build && electron-builder",
    "electron-start": "electron .",
    "electron-pack": "npm run build && electron-builder --dir",
    "electron-dist": "npm run build && electron-builder",
    "electron-dist-all": "npm run build && electron-builder -mwl",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@isomorphic-git/lightning-fs": "^4.6.0",
    "@modelcontextprotocol/sdk": "^1.6.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@types/crypto-js": "^4.2.2",
    "@types/uuid": "^10.0.0",
    "@uiw/react-md-editor": "^4.0.5",
    "@xenova/transformers": "^2.17.2",
    "@xyflow/react": "^12.4.4",
    "crypto-js": "^4.2.0",
    "electron-is-dev": "^3.0.1",
    "isomorphic-git": "^1.29.0",
    "jest": "^29.7.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "next": "15.1.7",
    "node-fetch": "^3.3.2",
    "openai": "^4.86.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "simple-git": "^3.27.0",
    "slate": "^0.112.0",
    "slate-history": "^0.110.3",
    "slate-react": "^0.112.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.26.1",
    "buffer": "^6.0.3",
    "concurrently": "^9.1.2",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "eslint-import-resolver-typescript": "^3.8.4",
    "eslint-plugin-import": "^2.31.0",
    "node-loader": "^2.1.0",
    "postcss": "^8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.1.5",
    "process": "^0.11.10",
    "typescript": "^5",
    "wait-on": "^8.0.2",
    "webpack": "^5.98.0"
  }
